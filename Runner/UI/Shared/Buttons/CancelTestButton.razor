@inject IPresentationService PresentationService;

<InlineButton @onclick="() => goToAsync()">Cancel</InlineButton>

@code {
    [Parameter]
    public IEnumerable<IFilterOrder> Filters { get; set; }

    async Task goToAsync()
    {
        await PresentationService.CancelTestAsync(new CancelTestRequest(Filters.ToArray()));
    }
}